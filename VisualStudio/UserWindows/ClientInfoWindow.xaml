<Window x:Class="VisualRedis.UserWindows.ClientInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualRedis.UserWindows"
        mc:Ignorable="d"
        Title="属性" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <TabControl>
        <TabItem Header="服务器信息">
            <ListView Name="clientProperty"/>
        </TabItem>
        <TabItem Header="客户端连接列表">
            <DataGrid Name="clientDataGrid"/>
        </TabItem>
    </TabControl>
</Window>
